{
  "parser": "babel-eslint",
  "rules": {
    "indent": 0,
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": 0,
    "semi": [
      2,
      "always"
    ],
    "no-reserved-keys": 0,
    "no-wrap-func": 0,
    "no-trailing-spaces": 0,
    "spaced-comment": 0,
    "eol-last": 0,
    "space-before-blocks": 0,
    "space-after-keywords": 0,
    "no-unused-vars": 0,
    // disabled dueto eslint error
    "no-duplicate-case": 0,
    "brace-style": 0,
    "func-names": 0,
    "react/jsx-boolean-value": 0,
    "object-curly-spacing": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": [
    "__CONFIG__"
  ],
  "extends": [
    "airbnb",
    "airplus"
  ],
  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "plugins": [
    "react"
  ]
}
